{
  "name": "react-weather-app",
  "version": "0.1.0",
  "description": "Simple weather app - exercise task for Free Code Camp course",
  "main": "lib/index.js",
  "license": "MIT",
  "repository": "petersandor/react-weather-app",
  "scripts": {
    "start": "webpack-dashboard -- node server.js",
    "clean": "rimraf lib dist coverage",
    "lint": "eslint src scripts *.js",
    "testonly": "cross-env NODE_ENV=test jest",
    "testonly:watch": "npm run testonly -- --watch",
    "test": "npm run lint && npm run testonly",
    "test:cov": "npm run lint && npm run testonly -- --coverage",
    "check": "npm run lint && npm run test ",
    "build": "npm run clean && cross-env NODE_ENV=production webpack src/index.js --config webpack.config.prod.js",
    "preversion": "npm run clean && npm run check",
    "version": "npm run build",
    "postversion": "git push && git push --tags && npm run clean",
    "prepublish": "npm run clean && npm run build",
    "postinstall": "node node_modules/fbjs-scripts/node/check-dev-engines.js package.json",
    "predeploy": "npm run clean && npm run build",
    "deploy": "gh-pages -d dist"
  },
  "author": {
    "name": "Peter Sandor",
    "email": "peter@petersandor.name",
    "url": "petersandor.name"
  },
  "devEngines": {
    "node": ">=4"
  },
  "keywords": [
    "redux",
    "react-weather-app"
  ],
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-webpack-loaders": "^0.9.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-2": "^6.18.0",
    "babel-register": "^6.18.0",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "enzyme": "^2.6.0",
    "eslint": "^4.3.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.1.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "fbjs-scripts": "^0.8.1",
    "file-loader": "^0.11.2",
    "gh-pages": "^1.0.0",
    "html-webpack-plugin": "^2.24.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.4",
    "json-loader": "^0.5.4",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
    "postcss-nested": "^2.1.0",
    "postcss-sass": "^0.2.0",
    "postcss-simple-vars": "^4.0.0",
    "react-addons-test-utils": "^15.4.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.6",
    "rimraf": "^2.5.4",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.7",
    "webpack": "^3.4.1",
    "webpack-bundle-analyzer": "^2.2.1",
    "webpack-dashboard": "^0.4.0",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.2"
  },
  "dependencies": {
    "@google/maps": "^0.4.3",
    "babel-core": "^6.25.0",
    "babel-helpers": "^6.24.1",
    "dark-sky-api": "^0.6.21",
    "material-ui": "^0.18.7",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.5",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.7",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "uuid": "^3.1.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.css$": "<rootDir>/__mocks__/styleMock.js"
    }
  }
}
